.section CODE
	mov 0x01, RA
	mov 0x02, RB
	mov 0x03, RC
	mov 0x04, RD
	sec 
	add RA, RB
	; Carry flag should be set
	jc 0x03
	; Carry flag should be set
	mov 0x01, RA
	add RA, RB
	; Carry flag should be cleared
	clc
	add RA, RB
	; Carry flag should be cleared
	jc 0x02
	; Carry flag should be cleared
	hlt 
	; RA = 0x05
	; RB = 0x02
	; RC = 0x03
	; RD = 0x04
	; SP = 0x21
	; SS = 0x21
	; [0x2100] = 0x01
	; [0x2101] = 0x02
	; [0x2102] = 0x03
	; [0x2103] = 0x04
	; C = 0
	; N = 0
	; Z = 0
	