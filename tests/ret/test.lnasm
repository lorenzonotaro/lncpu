.section CODE
	mov 0x21, SS
	mov 0x0, SP
	mov 0x21, RA
	push RA
	mov 0x22, RA
	push RA
	mov 0x23, RA
	push RA
	mov 0x01, RC
	mov 0x00, RD
	mov 0x20, [RC:RD]
	mov 0x21, [RC:RD+1]
	lcall RC:RD
	ret 
