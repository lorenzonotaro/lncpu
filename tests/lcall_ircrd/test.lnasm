.section CODE
	mov 0x21, SS
	mov 0x0, SP
	mov 0x01, RC
	mov 0x02, RD
	mov 0x03, [RC:RD]
	mov 0x04, [RC:RD+1]
	mov 0x05, [RC:RD+2]
	mov 0x06, [RC:RD+3]
	lcall [RC:RD]
	hlt 
	; RC = 0x01
	; RD = 0x02
	; [0x2100] = 0x06
	; [0x2101] = 0x05
	; [0x2102] = 0x04
	; [0x2103] = 0x03
	; PC = 0x03
	; SP = 0x21
	