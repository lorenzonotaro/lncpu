
extern void puts(char far* str);

extern void putc(char c);

extern void printhex(int n);

int mod(int a, int m){
    do{
        a = a - m;
    }while(a >= m);
    return a;
}


void fizzbuzz(int i) {
    printhex(i);

    putc(' ');

    if(mod(i, 3) == 0){
        puts("Fizz");
    }
    if(mod(i, 5) == 0){
        puts("Buzz");
    }

    putc('\n');
}

void main(){
    for(int i = 0; i < 16; ++i){
        fizzbuzz(i);
    }
}
