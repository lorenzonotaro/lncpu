extern void IOUTILS_PUTS(char far *s);
extern void IOUTILS_PUTBYTE(int n);
extern void IO_PUTC(char c);

int mod(int a, int m){
    // modulo without using %
    while(a >= m){
        a = a - m;
    }

    return a;
}

void fizzbuzz(int i){
    IOUTILS_PUTBYTE(i);

    IO_PUTC(' ');

    if(mod(i, 3) == 0){
        IOUTILS_PUTS("Fizz");
    }
    if(mod(i, 5) == 0){
        IOUTILS_PUTS("Buzz");
    }
}

void main(){
    fizzbuzz(15);
}
