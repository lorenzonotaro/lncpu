
extern void puts(char far* str);

extern void putc(char c);

extern void puti(int n);

int mod(int a, int m){
    do{
        a = a - m;
    }while(a >= m);
    return a;
}


void fizzbuzz(int i) {
    puti(i);

    putc(' ');

    if(mod(i, 3) == 0){
        puts("Fizz");
    }
    if(mod(i, 5) == 0){
        puts("Buzz");
    }

    putc('\n');
}
